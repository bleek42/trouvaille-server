{
	"name": "trouvaille",
	"version": "1.0.0",
	"description": "",
	"main": "server.js",
	"type": "module",
	"scripts": {
		"test": "mocha --require test/setup.js",
		"dev": "nodemon src/server.js",
		"migrate": "postgrator --config postgrator-config.js",
		"seed:1": "psql -U bleek -d trouvaille --file='seed/seed.users.sql'",
		"seed:2": "psql -U bleek -d trouvaille --file='seed/seed.interests.sql'",
		"seed:3": "psql -U bleek -d trouvaille --file='seed/seed.trips.sql'",
		"migrate:test": "env NODE_ENV=test npm run migrate",
		"migrate:production": "env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate",
		"start": "node src/server.js",
		"predeploy": "npm audit && npm run migrate:production",
		"deploy": "git push heroku master"
	},
	"keywords": [
		"travel",
		"road trip",
		"planner",
		"node js",
		"express",
		"postgresql",
		"knex",
		"api"
	],
	"author": "bleek42",
	"license": "ISC",
	"dependencies": {
		"@googlemaps/google-maps-services-js": "^3.3.13",
		"axios": "^0.26.1",
		"bcrypt": "^4.0.1",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"helmet": "^3.22.0",
		"jsonwebtoken": "^8.5.1",
		"knex": "^0.21.1",
		"morgan": "^1.10.0",
		"pg": "^8.0.3",
		"polyline": "^0.2.0",
		"postgrator": "^4.0.0",
		"prettier-eslint-cli": "^5.0.1",
		"xss": "^1.0.6"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"eslint": "^8.14.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-node": "^4.1.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"mocha": "^7.1.1",
		"nodemon": "^2.0.2",
		"postgrator-cli": "^3.2.0",
		"prettier": "^2.6.2",
		"prettier-eslint": "^14.0.2",
		"supertest": "^4.0.2"
	}
}
